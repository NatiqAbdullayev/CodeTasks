@model CarCreateVM

<form asp-area="Admin" asp-controller="Car" asp-action="Create" method="post" enctype="multipart/form-data">
	<div class="form-group">
		<label for="exampleInputEmail1">Full Car Name</label>
		<input asp-for="FullName" class="form-control" id="exampleInputEmail1" >
	</div>
	<div class="form-group">
		<label for="exampleInputEmail1">Brand</label>
		<input asp-for="Brand" class="form-control" >
	</div>
	<div class="form-group">
		<label for="exampleInputEmail1">Model Year</label>
		<input asp-for="ModelYear" class="form-control" >
	</div>
	<div class="form-group">
		<label for="exampleInputEmail1">Engine Power</label>
		<input asp-for="EnginePower" class="form-control" >
	</div>
	<div class="form-group">
		<label for="exampleInputEmail1">Transmission Type</label>
		<input asp-for="TransmissionType" class="form-control" >
	</div>
	<div class="form-group">
		<label for="exampleInputEmail1">Car Description</label>
		<input asp-for="CarDescription" class="form-control" >
	</div>
	<div class="form-group">
		<label for="exampleInputPassword1">Price</label>
		<input asp-for="Price" class="form-control" iv class="form-group">
